{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"]
      },
      "modules": false
    }],
    "react",
    "stage-1"
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }
    ]
  ],
  "compact": "true",
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
